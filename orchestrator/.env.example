# ==================================================
# orchestrator/.env.example
# Copy to orchestrator/.env and fill in values
# Runs on: Mac Studio
# ==================================================

# VPS API endpoint
VPS_URL=https://unsaltedbutter.ai
VPS_API_KEY=SAME_AS_AGENT_HMAC_SECRET_IN_WEB_ENV

# Mac Mini agent endpoint (LAN address)
MINI_URL=http://192.168.1.YYY:8421

# Inference model paths (on this machine)
MODEL_32B_PATH=/path/to/qwen3-vl-32b-q4.gguf
MODEL_8B_PATH=/path/to/qwen3-vl-8b-q4.gguf

# Inference API port
INFERENCE_PORT=8420

# Path to encryption key (same key as VPS, copied securely)
ENCRYPTION_KEY_PATH=/etc/unsaltedbutter/encryption.keyfile

# Daily batch pull time (cron-like, 24h format, EST)
BATCH_PULL_HOUR=5
BATCH_PULL_MINUTE=30
